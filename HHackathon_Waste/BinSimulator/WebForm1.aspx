<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="WebForm1.aspx.cs" Inherits="HHackathon_Waste.BinSimulator.WebForm1" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
        <div style="text-align:center;">
            <asp:Label ID="lblBin" runat="server" Text="Select Bin" />
            <br />
            <asp:DropDownList ID="ddlBin" runat="server" DataSourceID="SqlDataSource1" DataTextField="ID" DataValueField="ID" />
            <asp:SqlDataSource runat="server" ID="SqlDataSource1" ConnectionString='<%$ ConnectionStrings:ConnectionString %>' SelectCommand="SELECT [ID] FROM [WBin]"></asp:SqlDataSource>
            <br />
            <br />
            <asp:Label ID="lblLevel" runat="server" Text="Waste Level" />
            <br />
            <asp:TextBox ID="tbLevel" runat="server" />
            <br />
            <br />
            <asp:CheckBox ID="cbxSolid" runat="server" Text="Contains Solids" />
            <asp:CheckBox ID="cbxLiquid" runat="server" Text="Contains Liquids" />
            <br /><br />
            <asp:Label ID="lblTemp" runat="server" Text="Temperature" />
            <br />
            <asp:TextBox ID="tbTemp" runat="server" />
            <br /><br />
            <asp:Label ID="lblTilt" runat="server" Text="Tilt Level" />
            <br />
            <asp:TextBox ID="tbTilt" runat="server" />
            <br />
            <asp:ImageButton ID="addBtn" runat="server" ImageAlign="AbsBottom" ImageUrl="data:image/jpeg;base64," OnClick="addBtn_Click" />
        </div>
    </form>
</body>
</html>
